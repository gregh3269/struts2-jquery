{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "baseBranches": [
    "release/5.0.x",
    "release/6.0.x"
  ],
  "packageRules": [
    {
      "matchUpdateTypes": ["major","minor", "patch"],
      "matchCurrentVersion": "!/^0/",
      "automerge": true,
      "excludePackagePrefixes": [
        "org.springframework",
        "org.hibernate.validator"
      ]
    }
  ]
}
